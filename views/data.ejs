<table>
    <thead>
        <tr>
            <th>Patients ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date Of Birth</th>
            <th>Gender</th>
            <th>Language</th>
        </tr>
    </thead>
    <tbody>
       <% results.forEach(row =>{%>
        <tr>
            <td> <% row.patients_id %> </td>
            <td> <% row.first_name %> </td>
            <td> <% row.last_name %> </td>
            <td> <% row.date_of_birth %> </td>
            <td> <% row.gender %> </td>
            <td> <% row.language %> </td>
        </tr>
       <%});%>
    </tbody>

    <thead></thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Provider Specialty</th>
        </tr>
    </thead>
    <tbody>
       <% results.forEach(row =>{%>
        <tr>
            <td> <% row.first_name %> </td>
            <td> <% row.last_name %> </td>
            <td> <% row.provider_specialty %> </td>
        </tr>
       <%});%>
    </tbody>
</table>